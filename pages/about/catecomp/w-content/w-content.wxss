/* pages/about/catecomp/w-content/w-content.wxss */
.content{
  width: 100%;
  overflow: hidden;
  background-color:whitesmoke;
  padding-left: 10rpx;
  padding-top: 10rpx;
  padding-right: 10rpx;
}
.item{
  /* margin-right: 10rpx; */
  margin-bottom: 30rpx;
  width: 96%;
  position: relative;
  border-radius: 20rpx;
}
.img{
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  height: 300rpx;
  width: 100%;
  margin: 0;
  padding: 0;
}
.right{
  z-index: 1;
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  bottom: 30rpx;
  right: 60rpx;
}
.itemtitle{
  width: 100%;
  /* height: 120rpx; */
  margin-top: -10rpx;
  background-color: white;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  padding-top: 30rpx;
  padding-bottom: 35rpx;
  padding-left: 20rpx;
  font-weight: 700;
  font-size: 30rpx;
}